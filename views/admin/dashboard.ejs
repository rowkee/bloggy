<div class="admin-title">
  <h2>Posts</h2>
  <a href="/add-post" class="btn">Add New</a>
</div>

<ul class="admin-posts">
  <% data.forEach(cheese => { %>
  <li>
    <a href="/post/<%= cheese._id %>"> <%= cheese.title %> &nearr; </a>
  </li>
  <div class="admin-post-controls">
    <a href="/edit-post/<%= data._id %>" class="btn">Edit</a>
    <form action="/delete-post/<%= data._id %>?_method=DELETE" method="POST">
      <input type="submit" value="Delete" class="btn" />
    </form>
  </div>

  <% }) %>
</ul>
